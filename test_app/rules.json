{
    "rules": [
        {
            "field": "From",
            "predicate": "contains",
            "value": "noreply@github.com",
            "actions": ["mark_as_read"]
        },
        {
            "field": "Subject",
            "predicate": "equals",
            "value": "Security alert",
            "actions": ["move_to:IMPORTANT"]
        }
    ]
}
