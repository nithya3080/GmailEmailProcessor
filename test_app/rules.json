{
    "rules": [
        {
            "field": "From",
            "predicate": "contains",
            "value": "no-reply@accounts.google.com",
            "actions": ["mark_as_read"]
        },
        {
            "field": "Subject",
            "predicate": "equals",
            "value": "Nithya, you have new application updates this week",
            "actions": ["move_to:IMPORTANT"]
        }
    ]
}
